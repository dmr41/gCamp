<% @response_json.each do |project| %>
    <% if "#{project[:id]}" ==  "#{@test}"%>
      <% @project_name = "#{project[:name]}"%>
    <% end %>
<% end %>
<div class="page-header">
  <h1>
    <%= "Stories for #{@project_name}" %>
  </h1>
  <hr>
</div>
<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th class="col-md-6">
        Story
      </th>
      <th class="col-md-2 text-center">
        Estimate
      </th>
      <th class="col-md-2 text-center">
        Labels
      </th>
      <th class="col-md-2 text-center">
        Current State
      </th>
    </tr>
  </thead>
  <tbody>
    <% @story_response.each do |response| %>
    <tr>
      <td class="col-md-1">
        <%= link_to("#{response[:name]}", response[:url])%>
      </td>
      <td class="col-md-1 text-center">
        <%= response[:estimate]%>
      </td>
      <td class="col-md-2 text-center">
        <%response[:labels].each do |label|%>
          <%= label[:name] %>
          <% end %>
      </td>
      <td class="col-md-2 text-center">
        <%= response[:current_state]%>
      </td>
    </tr>
    <%end%>
  </tbody>
</table>
