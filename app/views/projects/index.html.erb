<div class="page-header">
  <h1 class="heading-shift">
    Projects
    <small class="pull-right">
    <%=link_to "Create project", new_project_path, class: "btn btn-info"  %>
    </small>
  </h1>
  <hr>
</div>
  <br>
  <table class="table">
    <thead>
      <tr>
        <th class="col-md-10">
          Name
        </th>

      </tr>
    </thead>
    <tbody>
      <% @projects.each do |project| %>
      <tr>
        <td class="col-md-12">
          <%= link_to("#{project.name}", project_path(project))%>
        </td>
        </td>
      </tr>
      <%end%>
    </tbody>
  </table>
